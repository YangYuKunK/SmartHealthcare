    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>bfd3639e-448f-4c24-b88c-d5387ffb923d</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
